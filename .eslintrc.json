{
    "env": {
        "browser": true,
        "node": true,
        // "commonjs": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["warn", "single"],
        "no-empty": "warn",
        "no-unused-vars": 1,
        "no-var": "warn",
        // "indent": ["warn", "tab"]
    }
}
